    </resultMap>

    <resultMap id="WmMovieWmMovieVideoResult" type="WmMovie" extends="WmMovieResult">
        <collection property="wmMovieVideoList" notNullColumn="sub_movie_video_id" javaType="java.util.List" resultMap="WmMovieVideoResult" />
    </resultMap>

    <resultMap type="WmMovieVideo" id="WmMovieVideoResult">
        <result property="movieVideoId"    column="sub_movie_video_id"    />
        <result property="title"    column="sub_title"    />
        <result property="url"    column="sub_url"    />
        <result property="ext"    column="sub_ext"    />
        <result property="length"    column="sub_length"    />
